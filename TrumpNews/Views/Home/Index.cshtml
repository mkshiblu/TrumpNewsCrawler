@model TrumpNews.Models.TrumpNewsViewModel

@{
    ViewBag.Title = "Home Page";
}

<div class="row">

    <div class="col-md-7">
        @foreach (var item in Model.Articles)
        {
            <div class="row">
                @Html.Partial("_ArticlePartial", item)
            </div>
        }
    </div>
    <div class="col-md-5">
        @foreach (var item in Model.Tweets)
        {
            <div class="row">
                @Html.Partial("_TweetPartial", item)
            </div>
        }
    </div>

</div>